////1st Assignment 
class student {
    int roll;
    String add;
    int phone;

    student(int roll, long phone, String add) {
        System.out.println("roll ");
        System.out.println(roll);
        System.out.println("phone ");
        System.out.println(phone);
        System.out.println("add ");
        System.out.println(this.add);
    }

}

public class assign1 {
    public static void main(String[] args) {

        System.out.println("name of student is:");
        System.out.println("sam");
        student obj1 = new student(23, 123456789, "nkt");
        System.out.println("name of student is:");
        System.out.println("john");
        student obj2 = new student(24, 1234567, "jaipur");

    }
}

////2nd Assignment
class Calculator {
    int num1;
    int num2;

    public Calculator() {
        this.num1 = 0;
        this.num2 = 0;
    }

    public int add(int num1, int num2) {
        return num1 + num2;
    }

    public int add() {
        return num1 + num2;
    }

    public int multiply(int num1, int num2) {
        return num1 * num2;
    }

    public int multiply() {
        return num1 * num2;
    }
}

public class assign2 {
    public static void main(String[] args) {
        Calculator calc = new Calculator();

        calc.num1 = 5;
        calc.num2 = 3;

        int sum1 = calc.add(10, 20);
        System.out.println("Sum (with parameters): " + sum1);

        int sum2 = calc.add();
        System.out.println("Sum (without parameters): " + sum2);

        int product1 = calc.multiply(4, 5);
        System.out.println("Product (with parameters): " + product1);

        int product2 = calc.multiply();
        System.out.println("Product (without parameters): " + product2);
    }
}

////3nd Assignment
public class Overloadingassign3 {
    private int num1;
    private int num2;

    public Overloadingassign3() {
        this.num1 = 0;
        this.num2 = 0;
    }

    public Overloadingassign3(int num1, int num2) {
        this.num1 = num1;
        this.num2 = num2;
    }

    public int add() {
        return num1 + num2;
    }

    public int add(int num3) {
        return num1 + num2 + num3;
    }

    public int add(int num3, int num4) {
        return num1 + num2 + num3 + num4;
    }

    public static void main(String[] args) {
        Overloadingassign3 example1 = new Overloadingassign3();
        Overloadingassign3 example2 = new Overloadingassign3(5, 10);

        System.out.println("Addition with no parameters: " + example1.add());
        System.out.println("Addition with two parameters: " + example2.add());
        System.out.println("Addition with three parameters: " + example2.add(15));
        System.out.println("Addition with four parameters: " + example2.add(20, 25));
    }
}
////4nd Assignment
public class Birdassign4 {
    private String name;

    public Birdassign4() {
        this.name = "Unknown Bird";
    }

    public Birdassign4(String name) {
        this.name = name;
    }

    public void displayBirdName() {
        System.out.println("Bird Name: " + name);
    }

    public static void main(String[] args) {
        Birdassign4 unknownBird = new Birdassign4();
        Birdassign4 parrot = new Birdassign4("Parrot");
        Birdassign4 sparrow = new Birdassign4("Sparrow");

        unknownBird.displayBirdName();
        parrot.displayBirdName();
        sparrow.displayBirdName();
    }
}
